---
import "../CSS/style.css";
---

<header>


</header>

<style>
  /* mobil */
  @media (max-width: 800px) {
    header {
      margin: 5%;
      display: flex;
      flex-direction: row;
      align-items: top;
      justify-content: space-between;
    }
  }

  /* desktop */
  @media (min-width: 800px) {
    header {
      margin: 2%;
      display: flex;
      flex-direction: row;
      align-items: top;
      justify-content: space-between;
    }
  }

</style>
